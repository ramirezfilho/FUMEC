#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int palRec(char xubango[5], int tamanho){
	if(tamanho == 1){
		return 1;
	}
	return 0;
}

int main(void) {
		char xubango['arara'];
		setbuf(stdout, NULL);
		printf("%d", palRec(xubango, 1));
		return EXIT_SUCCESS;
}
