#include <stdio.h>
#include <stdlib.h>

 int fibonacci(int n){
	 if(n == 0){
		 return 0;
	 }
	 else if(n == 1){
		 return 1;
	 } else {
		 n = (n-1) + (n-2);
		 return n;
	 }
 }


int main(void) {

	printf("%d", fibonnaci(2));



	return EXIT_SUCCESS;
}
