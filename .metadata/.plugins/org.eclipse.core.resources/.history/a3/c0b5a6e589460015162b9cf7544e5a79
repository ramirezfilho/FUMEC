package usuario;

import java.util.Scanner;
	

public class Main {

	public static void main(String[] args) {
	Scanner leia = new Scanner (System.in);	
		
		// Programa diagnóstico Fisioterapia
		int opcao = 0;
		int selecao = 0;
		
		System.out.println("Digite a parte do corpo: ");
		
		System.out.println("\n01 - Cabeça"
				+ "\n02 - Ombro"
				+ "\n03 - Joelho"
				+ "\n04 - Pé");
		
		opcao = leia.nextInt();
		
		switch (opcao) { 
			case 1: System.out.println("\nA) Olhos"
					+ "\nB) Ouvidos"
					+ "\nC) Boca"
					+ "\nD) Nariz");
			System.out.println("Digite a opçaõ desejada: " + (selecao = leia.nextInt()));
			
					switch (selecao){
					case 1: break;
					case 2:; break;
					case 3:; break;
					case 4:; break;
					}; break;
			
			
			
			
			
			case 2:; break;
			case 3:; break;
			case 4:; break;
		}
	
	}

}
